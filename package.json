{
  "name": "aska",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "serve": "yarn build && serve ./out",
    "aska-serve": "GITHUB_ACTIONS=true yarn build && serve ./",
    "start": "next start",
    "lint": "next lint",
    "fix": "next lint --fix",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:ci": "jest --runInBand",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "git config --local core.hooksPath .githooks && echo 'githooks installed' || echo 'githooks install failed'",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "autoprefixer": "^10.4.2",
    "cssnano": "^5.0.16",
    "next": "^12.3.1",
    "postcss": "^8.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.4.3",
    "react-responsive": "^9.0.2",
    "react-scroll-parallax": "^3.3.1",
    "react-spring": "^9.5.5",
    "react-twitter-widgets": "^1.11.0",
    "url-join": "^5.0.0",
    "webpack": "^5"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^17.0.10",
    "@types/react": "^18.0.21",
    "babel-loader": "^8.2.3",
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-storybook": "^0.6.4",
    "eslint-plugin-tailwindcss": "^3.3.2",
    "jest": "^27.4.7",
    "lint-staged": "^13.0.3",
    "plop": "^3.0.5",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "^0.1.1",
    "serve": "^14.0.1",
    "storybook-addon-next-router": "^4.0.0",
